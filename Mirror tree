class Node:
    def _init_(self, val):
        self.right = None
        self.data = val
        self.left = None
class Solution:
    def mirror(self, root):
        # Code here
        queue=[]
        queue.append(root)
        while queue:
            cur=queue.pop(0)
            if cur:
                left=cur.left
                right=cur.right
                cur.left=right
                cur.right=left
                queue.append(left)
                queue.append(right)
        return root
